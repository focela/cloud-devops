# Global configuration
S3_BUCKET=<YOUR_S3_BUCKET_NAME>
AWS_REGION=<YOUR_AWS_REGION>
BACKUP_PASSPHRASE=<YOUR_SECURE_PASSPHRASE>
AWS_ACCESS_KEY_ID=<YOUR_AWS_ACCESS_KEY_ID>
AWS_SECRET_ACCESS_KEY=<YOUR_AWS_SECRET_ACCESS_KEY>
APP_TIMEZONE=Asia/Ho_Chi_Minh

# Atlassian database configuration
ATL_DB_VER=14.10
ATL_DB_USER=atlassian
ATL_DB_PASS=<YOUR_SECURE_PASSWORD>
ATL_DB_NAMES=atlassian_db1,atlassian_db2,atlassian_db3

# GitLab database configuration
GIT_DB_VER=14-20230628
GIT_DB_USER=gitlab
GIT_DB_PASS=<YOUR_SECURE_PASSWORD>
GIT_DB_NAME=gitlabhq_production
GIT_DB_EXT=pg_trgm,btree_gist

# Proxy database configuration
PROXY_DB_VER=latest
PROXY_DB_USER=proxy_user
PROXY_DB_PASS=<YOUR_SECURE_PASSWORD>
PROXY_DB_ROOT_PASS=<YOUR_SECURE_ROOT_PASSWORD>
PROXY_DB_NAME=proxy_production

# Redis configuration
GIT_REDIS_VER=6.2.6

# Confluence configuration
CONF_VER=8.5.16
CONF_PROXY_NAME=localhost
CONF_PROXY_PORT=443
CONF_TOMCAT_PORT=8090
CONF_TOMCAT_SCHEME=http
CONF_TOMCAT_SECURE=false
CONF_HTTP_PORT_BIND=8090
CONF_CATALINA_OPTS=-javaagent:/opt/atlassian/confluence/atlassian-agent.jar

# Jira configuration
JIRA_VER=9.12.14
JIRA_PROXY_NAME=localhost
JIRA_PROXY_PORT=443
JIRA_TOMCAT_PORT=8080
JIRA_TOMCAT_SCHEME=http
JIRA_TOMCAT_SECURE=false
JIRA_HTTP_PORT_BIND=8080
JIRA_CATALINA_OPTS=-javaagent:/opt/atlassian/jira/atlassian-agent.jar

# Crowd configuration
CROWD_VER=6.1.1
CROWD_PROXY_NAME=localhost
CROWD_PROXY_PORT=443
CROWD_TOMCAT_PORT=8095
CROWD_TOMCAT_SCHEME=http
CROWD_TOMCAT_SECURE=false
CROWD_HTTP_PORT_BIND=8095
CROWD_CATALINA_OPTS=-javaagent:/opt/atlassian/crowd/atlassian-agent.jar

# GitLab core configuration
GIT_VER=17.0.1
GIT_SSH_PORT_BIND=10022
GIT_HTTP_PORT_BIND=10080
GIT_DEBUG=false
GIT_TIMEZONE=Asia/Ho_Chi_Minh
GIT_HTTPS=false
GIT_SSL_SELF_SIGNED=false
GIT_HOST=localhost
GIT_PORT=10080
GIT_SSH_PORT=10022
GIT_RELATIVE_URL_ROOT=
GIT_ROOT_EMAIL=<YOUR_ROOT_EMAIL>
GIT_ROOT_PASS=<YOUR_SECURE_PASSWORD>

# GitLab database configuration
GIT_DB_ADAPTER=postgresql
GIT_DB_HOST=gitlab_db
GIT_DB_PORT=5432
GIT_DB_USER=gitlab
GIT_DB_PASS=<YOUR_SECURE_PASSWORD>
GIT_DB_NAME=gitlabhq_production

# GitLab Redis configuration
GIT_REDIS_HOST=gitlab_redis
GIT_REDIS_PORT=6379

# GitLab notification settings
GIT_NOTIFY_BROKEN_BUILDS=true
GIT_NOTIFY_PUSHER=false
GIT_EMAIL=notifications@example.com
GIT_EMAIL_REPLY=noreply@example.com
GIT_INCOMING_EMAIL=reply@example.com

# GitLab backup configuration
GIT_BACKUP_SCHEDULE=daily
GIT_BACKUP_TIME=01:00

# GitLab SMTP configuration
GIT_SMTP_ENABLED=false
GIT_SMTP_DOMAIN=www.example.com
GIT_SMTP_HOST=smtp.gmail.com
GIT_SMTP_PORT=587
GIT_SMTP_USER=mailer@example.com
GIT_SMTP_PASS=password
GIT_SMTP_STARTTLS=true
GIT_SMTP_AUTH=login

# GitLab IMAP configuration
GIT_IMAP_ENABLED=false
GIT_IMAP_HOST=imap.gmail.com
GIT_IMAP_PORT=993
GIT_IMAP_USER=mailer@example.com
GIT_IMAP_PASS=password
GIT_IMAP_SSL=true
GIT_IMAP_STARTTLS=false

# GitLab OAuth settings
GIT_OAUTH_ENABLED=false
GIT_OAUTH_SSO_PROVIDER=
GIT_OAUTH_ALLOW_SSO=
GIT_OAUTH_BLOCK_AUTO_USERS=true
GIT_OAUTH_LINK_LDAP=false
GIT_OAUTH_LINK_SAML=false
GIT_OAUTH_EXTERNAL_PROVIDERS=

# GitLab OAuth CAS3 configuration
GIT_OAUTH_CAS3_LABEL=cas3
GIT_OAUTH_CAS3_SERVER=
GIT_OAUTH_CAS3_DISABLE_SSL=false
GIT_OAUTH_CAS3_LOGIN_URL=/cas/login
GIT_OAUTH_CAS3_VALIDATE_URL=/cas/p3/serviceValidate
GIT_OAUTH_CAS3_LOGOUT_URL=/cas/logout

# GitLab OAuth Google configuration
GIT_OAUTH_GOOGLE_API_KEY=
GIT_OAUTH_GOOGLE_APP_SECRET=
GIT_OAUTH_GOOGLE_RESTRICT_DOMAIN=

# GitLab OAuth Facebook configuration
GIT_OAUTH_FB_API_KEY=
GIT_OAUTH_FB_APP_SECRET=

# GitLab OAuth Twitter configuration
GIT_OAUTH_TWITTER_API_KEY=
GIT_OAUTH_TWITTER_APP_SECRET=

# GitLab OAuth GitHub configuration
GIT_OAUTH_GH_API_KEY=
GIT_OAUTH_GH_APP_SECRET=
GIT_OAUTH_GH_URL=
GIT_OAUTH_GH_VERIFY_SSL=

# GitLab OAuth GitLab configuration
GIT_OAUTH_GITLAB_API_KEY=
GIT_OAUTH_GITLAB_APP_SECRET=

# GitLab OAuth Bitbucket configuration
GIT_OAUTH_BITBUCKET_API_KEY=
GIT_OAUTH_BITBUCKET_APP_SECRET=
GIT_OAUTH_BITBUCKET_URL=

# GitLab OAuth SAML configuration
GIT_OAUTH_SAML_ACS_URL=
GIT_OAUTH_SAML_IDP_CERT=
GIT_OAUTH_SAML_SSO_URL=
GIT_OAUTH_SAML_ISSUER=
GIT_OAUTH_SAML_LABEL="SAML Provider"
GIT_OAUTH_SAML_NAME_ID_FORMAT=urn:oasis:names:tc:SAML:2.0:nameid-format:transient
GIT_OAUTH_SAML_GROUPS_ATTR=
GIT_OAUTH_SAML_EXT_GROUPS=
GIT_OAUTH_SAML_ATTR_EMAIL=
GIT_OAUTH_SAML_ATTR_NAME=
GIT_OAUTH_SAML_ATTR_USERNAME=
GIT_OAUTH_SAML_ATTR_FIRST_NAME=
GIT_OAUTH_SAML_ATTR_LAST_NAME=

# GitLab OAuth Crowd configuration
GIT_OAUTH_CROWD_SERVER=
GIT_OAUTH_CROWD_APP_NAME=
GIT_OAUTH_CROWD_APP_PASS=

# GitLab OAuth Auth0 configuration
GIT_OAUTH_AUTH0_ID=
GIT_OAUTH_AUTH0_SECRET=
GIT_OAUTH_AUTH0_DOMAIN=
GIT_OAUTH_AUTH0_SCOPE=

# GitLab OAuth Azure configuration
GIT_OAUTH_AZURE_KEY=
GIT_OAUTH_AZURE_SECRET=
GIT_OAUTH_AZURE_TENANT=

# Proxy configuration
PROXY_VER=2.12.1
PROXY_HTTP_PORT_BIND=80
PROXY_HTTPS_PORT_BIND=443
PROXY_ADMIN_PORT_BIND=81

# Proxy database configuration
PROXY_DB_VER=latest
PROXY_DB_HOST=proxy_db
PROXY_DB_PORT=3306
