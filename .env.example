# MySQL Database Configuration for npm_db
NPM_DB_USER=npm
NPM_DB_NAME=npm_db_name
NPM_DB_PASSWORD=npm_password
NPM_DB_ROOT_PASSWORD=npm_root_password

# MySQL Configuration for proxy_manager
DB_MYSQL_HOST=npm_db
DB_MYSQL_PORT=3306
DB_MYSQL_USER=npm
DB_MYSQL_NAME=npm_production
DB_MYSQL_PASSWORD=npm_password

# GitLab configuration
GITLAB_SSH_PORT=10022
GITLAB_HOST=localhost
GITLAB_PORT=10080
GITLAB_HTTPS=false
SSL_SELF_SIGNED=false
GITLAB_TIMEZONE=UTC

# GitLab secrets (You should replace these with secure and unique values)
GITLAB_SECRETS_DB_KEY_BASE=long-and-random-alphanumeric-string
GITLAB_SECRETS_SECRET_KEY_BASE=long-and-random-alphanumeric-string
GITLAB_SECRETS_OTP_KEY_BASE=long-and-random-alphanumeric-string

# GitLab root account (Change to your desired email and password)
GITLAB_ROOT_EMAIL=admin@example.com
GITLAB_ROOT_PASSWORD=adminpassword

# GitLab database configuration
DB_HOST=gitlab_db
DB_PORT=5432
DB_USER=gitlab
DB_NAME=gitlabhq-production
DB_PASS=securepassword

# Redis configuration for GitLab
REDIS_HOST=gitlab_redis
REDIS_PORT=6379

# Backup configuration
GITLAB_BACKUP_SCHEDULE=daily
GITLAB_BACKUP_TIME=01:00

# Email configuration (SMTP settings for email notifications)
SMTP_ENABLED=false
SMTP_DOMAIN=www.example.com
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=mailer@example.com
SMTP_PASS=securepassword
SMTP_STARTTLS=true
SMTP_AUTHENTICATION=login

# IMAP configuration (if using incoming email features)
IMAP_ENABLED=false
IMAP_HOST=imap.gmail.com
IMAP_PORT=993
IMAP_USER=mailer@example.com
IMAP_PASS=securepassword
IMAP_SSL=true
IMAP_STARTTLS=false

# OAuth configuration (if using any OAuth providers)
OAUTH_ENABLED=false
OAUTH_GOOGLE_API_KEY=
OAUTH_GOOGLE_APP_SECRET=
OAUTH_GOOGLE_RESTRICT_DOMAIN=
OAUTH_GITHUB_API_KEY=
OAUTH_GITHUB_APP_SECRET=
OAUTH_GITHUB_URL=
OAUTH_GITHUB_VERIFY_SSL=true
OAUTH_GITLAB_API_KEY=
OAUTH_GITLAB_APP_SECRET=
OAUTH_BITBUCKET_API_KEY=
OAUTH_BITBUCKET_APP_SECRET=
OAUTH_BITBUCKET_URL=
