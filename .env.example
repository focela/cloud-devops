# MySQL Database Configuration for npm_db
NPM_DB_USER=npm
NPM_DB_NAME=npm_db_name
NPM_DB_PASSWORD=npm_password
NPM_DB_ROOT_PASSWORD=npm_root_password

# MySQL Configuration for proxy_manager
DB_MYSQL_HOST=npm_db
DB_MYSQL_PORT=3306
DB_MYSQL_USER=npm
DB_MYSQL_NAME=npm_production
DB_MYSQL_PASSWORD=npm_password

# General GitLab configuration
DEBUG=false

# Database configuration
DB_ADAPTER=postgresql
DB_HOST=gitlab_db
DB_PORT=5432
DB_USER=gitlab
DB_NAME=gitlabhq_production
DB_PASS=secure_password

# Redis configuration
REDIS_HOST=gitlab_redis
REDIS_PORT=6379

# Timezone configuration
TZ=Asia/Ho_Chi_Minh
GITLAB_TIMEZONE=Asia/Ho_Chi_Minh

# GitLab configuration
GITLAB_HTTPS=false
SSL_SELF_SIGNED=false
GITLAB_HOST=localhost
GITLAB_PORT=10080
GITLAB_SSH_PORT=10022
GITLAB_RELATIVE_URL_ROOT=
GITLAB_SECRETS_DB_KEY_BASE=long-and-random-alphanumeric-string
GITLAB_SECRETS_SECRET_KEY_BASE=long-and-random-alphanumeric-string
GITLAB_SECRETS_OTP_KEY_BASE=long-and-random-alphanumeric-string

# GitLab root user credentials
GITLAB_ROOT_PASSWORD=
GITLAB_ROOT_EMAIL=

# GitLab notifications
GITLAB_NOTIFY_ON_BROKEN_BUILDS=true
GITLAB_NOTIFY_PUSHER=false

# GitLab email configuration
GITLAB_EMAIL=notifications@example.com
GITLAB_EMAIL_REPLY_TO=noreply@example.com
GITLAB_INCOMING_EMAIL_ADDRESS=reply@example.com

# GitLab backup configuration
GITLAB_BACKUP_SCHEDULE=daily
GITLAB_BACKUP_TIME=01:00

# SMTP configuration
SMTP_ENABLED=false
SMTP_DOMAIN=www.example.com
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=mailer@example.com
SMTP_PASS=password
SMTP_STARTTLS=true
SMTP_AUTHENTICATION=login

# IMAP configuration (for incoming email)
IMAP_ENABLED=false
IMAP_HOST=imap.gmail.com
IMAP_PORT=993
IMAP_USER=mailer@example.com
IMAP_PASS=password
IMAP_SSL=true
IMAP_STARTTLS=false

# OAuth configuration
OAUTH_ENABLED=false
OAUTH_AUTO_SIGN_IN_WITH_PROVIDER=
OAUTH_ALLOW_SSO=
OAUTH_BLOCK_AUTO_CREATED_USERS=true
OAUTH_AUTO_LINK_LDAP_USER=false
OAUTH_AUTO_LINK_SAML_USER=false
OAUTH_EXTERNAL_PROVIDERS=

# CAS3 configuration (if using CAS3 for authentication)
OAUTH_CAS3_LABEL=cas3
OAUTH_CAS3_SERVER=
OAUTH_CAS3_DISABLE_SSL_VERIFICATION=false
OAUTH_CAS3_LOGIN_URL=/cas/login
OAUTH_CAS3_VALIDATE_URL=/cas/p3/serviceValidate
OAUTH_CAS3_LOGOUT_URL=/cas/logout

